{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "simple-interactive-blocks/simple-interactive-blocks",
	"version": "0.1.0",
	"title": "Simple interactive blocks",
	"category": "widgets",
	"icon": "media-interactive",
	"description": "An interactive block with the Interactivity API.",
	"example": {},
	"supports": {
		"interactivity": true,
		"anchor": true,
		"customClassName": true,
		"align": [ "left", "right", "full", "wide"],
		"alignWide": true,
		"color": {
			"gradients": true,
			"background": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"layout": true,
		"lock": true,
		"renaming": true
	},
	"attributes": {
		"style": {
			"type": "object",
			"default": {
				"color": {
					"background": "#fbf9f4"
				},
				"spacing": {
					"padding": {
						"top": 0,
						"right": 0,
						"botton": 0,
						"left": 0
					},
					"margin": {
						"top": 0,
						"right": 0,
						"botton": 0,
						"left": 0
					}
				}
			}
		},
		"placeholder": {
			"type": "string",
			"default": "Que est√°s buscando?"
		}
	},
	"textdomain": "simple-interactive-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.js"
}
